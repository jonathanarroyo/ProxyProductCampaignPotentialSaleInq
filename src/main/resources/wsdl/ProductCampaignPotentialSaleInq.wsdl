      <xsd:element name="PastDuePmtCount" type="Long_Type"/>
      <xsd:element name="PassbkNextLine" type="Long_Type"/>
      <xsd:element name="OriginatorType" type="Long_Type"/>
      <xsd:element name="Min" type="Long_Type"/>
      <xsd:element name="MatchedRec" type="Long_Type"/>
