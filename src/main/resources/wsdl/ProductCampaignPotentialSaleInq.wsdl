        <xsd:schema attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="urn://bancodebogota.com/customers/arrangement/v1/" xmlns:bbog="urn://bancodebogota.com/ifx/base/v1/" xmlns:bons0="urn://bancodebogota.com/ifx/base/v1/" xmlns:bons01="urn://bancodebogota.com/customers/arrangement/v1/" xmlns:bons02="urn://bancodebogota.com/customers/arrangement/v1/" xmlns:bons1="urn://bancodebogota.com/customers/arrangement/v1/">
      <xsd:import namespace="urn://bancodebogota.com/ifx/base/v1/"/>
      <xsd:annotation>
		<xsd:documentation>
			Contiene la informacion general de una campaña. Asociada a
			productos y clientes.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="CampaignInfoList" type="bons1:CampaignInfoList_Type">
	</xsd:element>
      <xsd:complexType name="CampaignInfo_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="bbog:Identifier">
				<xsd:annotation>
					<xsd:documentation>
						Identificacion de la campaña.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bons1:CampaignType"/>
			<xsd:element minOccurs="0" ref="bbog:Desc">
				<xsd:annotation>
					<xsd:documentation>
						Descripcion campaña
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:Status"/>
			<xsd:element minOccurs="0" ref="bbog:SelRangeDt">
				<xsd:annotation>
					<xsd:documentation>
						Rango de fecha inicio y vencimiento
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:MaxCurAmt">
				<xsd:annotation>
					<xsd:documentation>
						Total valor aprobado
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:NetworkTrnInfo">
				<xsd:annotation>
					<xsd:documentation>
						Informacion general del canal
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:RatingId">
				<xsd:annotation>
					<xsd:documentation>
						Score acierta con el que estudiaron el cliente
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:RefType">
				<xsd:annotation>
					<xsd:documentation>
						Score acierta con el que estudiaron el cliente
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="bons1:CampAccInfo">
				<xsd:annotation>
					<xsd:documentation>
						Informacion de los productos asociados a la campaña
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="bbog:CreditInfo">
				<xsd:annotation>
					<xsd:documentation>
						Informacion de obligaciones asociadas a la
						campaña.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:OriginatorName">
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:Income"/>
			<xsd:element minOccurs="0" ref="bbog:LOCLimit"/>
			<xsd:element minOccurs="0" ref="bbog:RemainingPmtCount"/>
			<xsd:element minOccurs="0" ref="bbog:Count"/>

			<xsd:element minOccurs="0" ref="bons1:CampaignRuleList"/>
		</xsd:sequence>
	</xsd:complexType>
      <xsd:complexType name="CampaignInfoList_Type">
		<xsd:sequence>

			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="bons1:CampaignInfo">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
      <xsd:element name="CampaignInfo" type="bons1:CampaignInfo_Type"/>
      <xsd:complexType name="CampaignRule_Type">
		<xsd:sequence>
			<xsd:element minOccurs="1" ref="bons0:RefType"/>
			<xsd:element minOccurs="0" ref="bons0:RefId"/>
			<xsd:element minOccurs="1" ref="bons0:AcctDomainList"/>
		</xsd:sequence>
	</xsd:complexType>
      <xsd:complexType name="CampaignRuleList_Type">
    	<xsd:sequence>
    		<xsd:element maxOccurs="unbounded" minOccurs="0" ref="bons1:CampaignRule"/>
    	</xsd:sequence>
    </xsd:complexType>
      <xsd:element name="CampaignRuleList" type="bons1:CampaignRuleList_Type"/>
      <xsd:element name="CampaignRule" type="bons1:CampaignRule_Type"/>
      <xsd:simpleType name="CampaignType_Type">
		<xsd:annotation>
      <xsd:documentation>En este campo se parametriza el Tipo de campaña</xsd:documentation>
    </xsd:annotation>
		<xsd:restriction base="Q1:NC">
			<xsd:maxLength value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:element name="CampaignType" type="bons01:CampaignType_Type"/>
      <xsd:annotation>
		<xsd:documentation>
			Información de los productos asociados a la campaña.
			Dirigida a clientes potenciales del banco.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="CampAccInfo" type="bons02:CampAccInfo_Type"/>
      <xsd:complexType name="CampAccInfo_Type">
		<xsd:complexContent>
			<xsd:extension base="bbog:Product_Type">
				<xsd:sequence>
					<xsd:element minOccurs="0" ref="bbog:AcctId">
						<xsd:annotation>
							<xsd:documentation>

							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:LinkedAcct">
						<xsd:annotation>
							<xsd:documentation>

							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:Franchise">
						<xsd:annotation>
							<xsd:documentation>

							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:ContactInfo">
						<xsd:annotation>
							<xsd:documentation>
								Informacion de contacto punto de entrega
								de informacion de la campaña.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:SelRangeChkNum">
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:CurAmt">
						<xsd:annotation>
							<xsd:documentation>
								En este campo valor del producto,tasa y
								plazo aprobado asociado a la campaña.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:ProductStatus">
						<xsd:annotation>
							<xsd:documentation>
								En este campo contiene informacion del
								producto asociado a a campaña.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:CardPrefId">
						<xsd:annotation>
							<xsd:documentation>
								En este campo contiene indicador de
								tarjeta de crédito realzada.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:IntRateInfo"/>

					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="bbog:RefInfo"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
    </xsd:schema>
        <xsd:schema attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="http://Messaging/customers/entities/arrangement" xmlns:Q1="http://Messaging/customers/entities/arrangement" xmlns:Q2="urn://bancodebogota.com/customers/arrangement/v1/" xmlns:Q3="urn://bancodebogota.com/creditcard/product/v1/" xmlns:Q4="http://Messaging/customers/entities/product" xmlns:bons0="urn://bancodebogota.com/ifx/base/v1/">
      <xsd:import namespace="urn://bancodebogota.com/customers/arrangement/v1/"/>
      <xsd:import namespace="urn://bancodebogota.com/creditcard/product/v1/"/>
