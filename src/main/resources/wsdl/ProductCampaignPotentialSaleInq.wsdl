		</xsd:documentation>
	</xsd:annotation>
      <xsd:simpleType name="LegalName_Type">
		<xsd:restriction base="bbog:C">
			<xsd:maxLength value="96"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:element name="LegalName" type="bbog:LegalName_Type"/>
      <xsd:annotation>
	    <xsd:documentation>
