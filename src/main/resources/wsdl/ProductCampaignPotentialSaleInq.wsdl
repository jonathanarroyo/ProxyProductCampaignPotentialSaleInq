		<xsd:restriction base="bbog:C">
			<xsd:maxLength value="96"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:annotation>
		<xsd:documentation>
			En este campo se parametriza la licencia de conducción.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:complexType name="DriversLicense_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="LicenseNum">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el número de licencia de conducción.
					</xsd:documentation>
				</xsd:annotation>			
			</xsd:element>
			<xsd:element minOccurs="0" ref="StateProv">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el código de provincia o estado.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="Country">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el país, el cual se basa en la norma ISO 3166, el tamaño 
	    				es de tres caracteres y aparece en las direcciones postales.
					</xsd:documentation>
				</xsd:annotation>			
			</xsd:element>
			<xsd:element minOccurs="0" ref="StatusCode">
				<xsd:annotation>
					<xsd:documentation>
						En este campo estan paramentrizados todos los posibles estados en 
	    				los cuales se puede encontrar una cuenta perteneciente a la entidad.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
