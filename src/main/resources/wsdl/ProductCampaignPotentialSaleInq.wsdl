			<xsd:element minOccurs="0" ref="TypeId">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el tipo de
						identificación, de cliente o persona natural o
						jurídica.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="ParticipantId">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el número de
						identificación.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="Role">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el identificador de
						los roles en que participa.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="PersonName">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el nombre de la
						persona y opcionalmente el sobrenombre de ésta.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="Gender">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el género.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="MaritalStatus">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el estado civil.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:City">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la ciudad
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:StateProv">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el código de provincia o estado.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>			
			<xsd:element minOccurs="0" ref="Country">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el país, el cual se
						basa en la norma ISO 3166, el tamaño es de tres
						caracteres y aparece en las direcciones
						postales.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="BirthDt">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la fecha de
						nacimiento.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="DriversLicense">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la licencia de
						conducción.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="ContactInfo">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la información de
						contacto.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" ref="FinantialInfo">
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" ref="ResidencePlace">
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" ref="EducationalLevel">
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" ref="DeathCertificateInfo">
				<xsd:annotation>
					<xsd:documentation>
						Agrupa informacion relevante de acta defuncion de una persona.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
      <xsd:annotation>
		<xsd:documentation>
			En este campo se parametriza el tipo de identificación, de cliente o persona natural o jurídica.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:simpleType name="TypeId_Type">
		<xsd:restriction base="NC">
			<xsd:maxLength value="3"/>
