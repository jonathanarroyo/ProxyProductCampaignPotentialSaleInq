      <xsd:import namespace="http://Messaging/customers/entities/product"/>
      <xsd:complexType name="CampPotentialSaleInq_Type">
    <xsd:sequence>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="bons0:AcctId"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="bons0:AgentId"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="bons0:ProductCode"/>
