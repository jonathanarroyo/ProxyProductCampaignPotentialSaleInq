    </xsd:sequence>
    </xsd:complexType>
      <xsd:element name="CampPotentialSaleInq" type="Q1:CampPotentialSaleInq_Type">
    </xsd:element>
    </xsd:schema>
        <xsd:schema attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="http://Messaging/customers/entities/product" xmlns:Q1="http://Messaging/customers/entities/product" xmlns:Q2="urn://bancodebogota.com/ifx/base/v1/" xmlns:Q3="urn://bancodebogota.com/customers/condition/v1/">
      <xsd:import namespace="urn://bancodebogota.com/customers/condition/v1/"/>
      <xsd:import namespace="urn://bancodebogota.com/ifx/base/v1/"/>
      <xsd:complexType name="ProductPotentialSale_Type">
    <xsd:sequence>
    	<xsd:element maxOccurs="unbounded" minOccurs="0" ref="Q2:ProductNum">
    	</xsd:element>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:AcctDomain"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:ProductId"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:RefId"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:BalAmt"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:CashAmt"/>
    	<xsd:element maxOccurs="unbounded" minOccurs="0" ref="Q2:Amt"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:EndDt"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:ProductCode"/>

    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:Desc"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:StatusDesc"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:CloseDt"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:Rate"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:RateInfoType"/>

    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:Term"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:Sales"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:CurAmt"/>
    	<xsd:element maxOccurs="1" minOccurs="0" ref="Q2:Indicator"/>

            <xsd:element maxOccurs="1" minOccurs="0" ref="Q2:Observation"/>
        </xsd:sequence>
