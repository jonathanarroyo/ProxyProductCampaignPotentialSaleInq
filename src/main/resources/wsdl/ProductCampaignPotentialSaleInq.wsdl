			<xsd:element maxOccurs="1" minOccurs="0" ref="StateProv"/>
            <xsd:element minOccurs="0" ref="Country"/>
            <xsd:element minOccurs="0" ref="Desc">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la descripción.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>
      <xsd:annotation>
		<xsd:documentation>
			En este campo se parametriza la información
			detallada de la facturación.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:complexType name="ResidencePlace_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="SocialStratum">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza las ventas o
						ingresos anuales.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" ref="StatusDesc"/>
			<xsd:element maxOccurs="1" minOccurs="0" ref="DwellingType"/>
		</xsd:sequence>
	</xsd:complexType>
      <xsd:element name="ResidencePlace" type="ResidencePlace_Type"/>
      <xsd:annotation>
		<xsd:documentation>
			En este campo se parametriza la información detallada de la
			facturación.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:complexType name="FinantialInfo_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="Income">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza las ventas o
						ingresos anuales.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="Outcome">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza los egresos
						anuales.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="TotalLiabilities">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza total pasivos.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="TotalAssets">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza total activos.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="OtherAssets"/>
			<xsd:element minOccurs="0" ref="OtherIncome">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza otros ingresos.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="OtherIncomeDesc">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la descripción de
						otros ingresos.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" ref="EffDt"/>
		</xsd:sequence>
