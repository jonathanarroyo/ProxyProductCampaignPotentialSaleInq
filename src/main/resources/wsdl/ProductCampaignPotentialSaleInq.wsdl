      <xsd:element name="PassbkLinesPerPage" type="Long_Type"/>
      <xsd:element name="NumEmployees" type="Long_Type"/>
      <xsd:element name="Max" type="Long_Type"/>
      <xsd:element name="Dependents" type="Long_Type"/>
      <xsd:element name="HistRetentionDays" type="Long_Type"/>
      <xsd:annotation>
