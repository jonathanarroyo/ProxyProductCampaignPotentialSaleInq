      <xsd:element name="ProductPotentialSale" type="Q1:ProductPotentialSale_Type">
  </xsd:element>
    </xsd:schema>
        <xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="urn://bancodebogota.com/security/involvedparty/v1/" xmlns="urn://bancodebogota.com/security/involvedparty/v1/" xmlns:bbog="urn://bancodebogota.com/ifx/base/v1/">
      <xsd:import namespace="urn://bancodebogota.com/ifx/base/v1/"/>
      <xsd:annotation>
		<xsd:documentation>
			En este campo se parametriza la información del usuario que autentica el servicio.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:complexType name="CustId_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="bbog:SPName">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el nombre del proveedor de servicio.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:sequence>
				<xsd:element minOccurs="0" ref="bbog:CustPermId">
					<xsd:annotation>
						<xsd:documentation>
							En este campo se parametriza ID permanente del cliente, usado como una 
	    					llave de base de datos para identificar de forma única un intercambio
	    					financiero o cliente CSP. No puede ser modificado por el cliente.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element minOccurs="0" ref="bbog:CustLoginId">
					<xsd:annotation>
						<xsd:documentation>
							En este campo se parametriza el ID inicio sesión del cliente.
							Usuario STA Campo de tres partes, el tercero  muestra el ID 
							del usuario que realizó la modificación.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:element minOccurs="0" ref="bbog:CustType">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el tipo de cliente.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:PersonalIdent"/>	
		</xsd:sequence>
	</xsd:complexType>
      <xsd:element name="UserId" type="CustId_Type"/>
      <xsd:element name="CustId" type="CustId_Type"/>
    </xsd:schema>
        <xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="urn://bancodebogota.com/creditcard/product/v1/" xmlns:Cc="urn://bancodebogota.com/creditcard/product/v1/" xmlns:bbog="urn://bancodebogota.com/ifx/base/v1/">
      <xsd:import namespace="urn://bancodebogota.com/ifx/base/v1/"/>
      <xsd:annotation>
		<xsd:documentation>
			Información del estado de la cuenta.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="AccountStatus" type="Cc:AccountStatus_Type"/>
      <xsd:complexType name="AccountStatus_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="bbog:StatusCode">
				<xsd:annotation>
					<xsd:documentation>
						En este campo estan paramentrizados todos los posibles estados en 
	    				los cuales se puede encontrar una cuenta perteneciente a la entidad.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="bbog:StatusDesc">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la descripción del estado.
					</xsd:documentation>
				</xsd:annotation>			
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="bbog:LockId">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el código de bloqueo, el cual es definido por la entidad. 
	    				En este caso, el codigo de bloqueo que muestra corresponde a los bloqueos que se pueden 
	    				aplicar desde oficina.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:EffDt">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la fecha efectiva en el estado de la cuenta.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:ExpDt">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la fecha vencimiento en el estado de la cuenta.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
      <xsd:annotation>
		<xsd:documentation>
			Información de la cuenta.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="Account" type="Cc:Account_Type"/>
      <xsd:element name="AccountIdFrom" type="Cc:Account_Type"/>
      <xsd:complexType name="Account_Type">
		<xsd:complexContent>
			<xsd:extension base="bbog:Product_Type">
				<xsd:sequence>
					<xsd:element minOccurs="0" ref="bbog:AcctId">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza el identificador de cuenta, el cual es un elemento de datos 
								que contiene una serie de números y/o caracteres que identifican una cuenta de cliente. 
								A veces se conoce como número de cuenta.
							</xsd:documentation>
						</xsd:annotation>						
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:LinkedAcct">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza el tipo de cuenta, el cual es el código Identificador del 
								tipo de Tarjeta solicitado, el cual define si requiere o no codeudor o amparado.
							</xsd:documentation>
						</xsd:annotation>											
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:AcctPlanCode">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza el código del plan asignado a la cuenta.
							</xsd:documentation>
						</xsd:annotation>											
					</xsd:element>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="bbog:PCTCode">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza el código identificador de las condiciones financieras de la cuenta.
							</xsd:documentation>
						</xsd:annotation>											
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:RatingId">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza el código de riesgo crediticio que la entidad 
	    						le puede asignar a una cuenta, viene de FV.
							</xsd:documentation>
						</xsd:annotation>											
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:EffDt">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza la fecha efectiva en la cuenta.
							</xsd:documentation>
						</xsd:annotation>											
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:ExpDt">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza la fecha vigencia en la cuenta.
							</xsd:documentation>
						</xsd:annotation>											
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:BillCycle">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza el ciclo de facturación, día en el cual se debe efectuar la facturación.
							</xsd:documentation>
						</xsd:annotation>											
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:BillCycleType">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza el tipo de ciclo de facturación.
							</xsd:documentation>
						</xsd:annotation>											
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:ForeignUse">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza el uso extranjero.
							</xsd:documentation>
						</xsd:annotation>											
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:CurAmt">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza el valor monetario en la cuenta.
							</xsd:documentation>
						</xsd:annotation>											
					</xsd:element>
					<xsd:element minOccurs="0" ref="bbog:BankInfo">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza la información adicional del banco usada para
								identificar éste en transferencias de pago, beneficiario, es obligatoria en 
								CHE, DEU, e ITA para identificar por completo una cuenta.
