      <xsd:annotation>
		<xsd:documentation>
			Este objeto se utiliza como contenedor general para los mensajes de error de la plataforma SOA. 
			Incluye internamente el agregado status de IFX.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="GeneralException" type="GeneralException_Type"/>
      <xsd:complexType name="GeneralException_Type">
		<xsd:sequence>
			<xsd:element minOccurs="1" ref="RqUID">
