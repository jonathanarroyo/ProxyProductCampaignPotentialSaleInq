					<xsd:documentation>
						En este campo se parametriza el apellido del cliente.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="FirstName">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el nombre del cliente.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="MiddleName">
