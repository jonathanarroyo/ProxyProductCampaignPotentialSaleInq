      <xsd:element name="DriversLicense" type="DriversLicense_Type"/>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza el número de licencia del conductor.	    	
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="LicenseNum" type="LicenseNum_Type"/>
      <xsd:simpleType name="LicenseNum_Type">
		<xsd:restriction base="C">
			<xsd:maxLength value="32"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:annotation>
		<xsd:documentation>
			En este campo se parametriza otros documentos de identificación.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="OtherIdentDoc" type="OtherIdentDoc_Type"/>
      <xsd:complexType name="OtherIdentDoc_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="TypeId">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el tipo de
						identificación, de cliente o persona natural o
						jurídica.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="ParticipantId">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el número de
						Identificación.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" ref="IssDt"/>
			<xsd:element minOccurs="0" ref="ExpDt">
